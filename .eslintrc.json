{
    "env": {
        "browser": true,
        "node":true,
        "es6":true,
        "jquery":true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
      
              "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": 0,
        "no-console":1,
        "no-var":1
    }
}